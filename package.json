{
  "name": "@crisellevaldez/speculo-ui",
  "private": false,
  "version": "0.7.18",
  "type": "module",
  "main": "./dist/speculo-ui.cjs",
  "module": "./dist/speculo-ui.mjs",
  "types": "./dist/index.d.ts",
  "style": "./dist/styles.css",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crisellevaldez/speculo-ui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/speculo-ui.mjs",
      "require": "./dist/speculo-ui.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "eslint . --fix && tsc && vite build",
    "build:docs": "vite build --config vite.docs.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "firebase": "^11.1.0",
    "lucide-react": "^0.468.0",
    "moment-timezone": "^0.5.46",
    "react-phone-number-input": "^3.4.10",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "latest",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.4.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  }
}
